      "source": [
        "print(\"Ingrese un numero\")\n",
        "numero = int(input(\"Ingrese un numero:\"))\n",
        "contador = 0\n",
        "for i in range(1, numero+1):\n",
        "  if i % 2 == 0:\n",
        "    contador += 1\n",
        "    print(i)\n",
        "print(\"hay\", contador, \"numero pares entre 1 y\", numero)"
